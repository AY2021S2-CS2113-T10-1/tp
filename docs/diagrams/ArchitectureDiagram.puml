@startuml
!include <office/Concepts/globe_internet>
!include <office/Concepts/documents>
!include <office/Users/user>
!include style.puml

Package " "<<Rectangle>>{
    Class UI UI_COLOR
    Class Parser LOGIC_COLOR
    Class Storage STORAGE_COLOR
    Class Command MODEL_COLOR
    Class Main #grey
    Class Food LOGIC_COLOR_T2
}

Class "<$user>" as User MODEL_COLOR_T2
Class "<$documents>" as File UI_COLOR_T2


UI -[#green]> Parser
Parser -[#blue]-> Command
Command -> Main
Main -[#grey]-> UI
Main -[#grey]-> Storage

Storage .right[STORAGE_COLOR].>File
User ..> UI
@enduml

@enduml